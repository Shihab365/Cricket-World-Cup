<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#d32f2f"
        tools:context="com.pro1.asus.cric_pro_1.Nz_player_Activity">

        <android.support.design.widget.AppBarLayout
                android:id="@+id/app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            android:fitsSystemWindows="true"
        >
            <android.support.design.widget.CollapsingToolbarLayout
                android:layout_width="match_parent"
                android:layout_height="315dp"
                app:layout_scrollFlags="scroll|exitUntilCollapsed"
                app:contentScrim="?attr/colorPrimary"
                app:expandedTitleTextAppearance="@android:color/transparent"
                android:fitsSystemWindows="true"
            >
                <ImageView
                        android:layout_gravity="center_horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="315dp"
                        android:src="@drawable/nz_flag"
                        android:scaleType="centerCrop"
                        android:contentDescription="@string/app_name"
                        app:layout_collapseMode="parallax"
                />
                <android.support.v7.widget.Toolbar
                        android:layout_width="match_parent"
                        android:layout_height="?attr/actionBarSize"
                        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                        app:title="Team New zealand"
                        app:layout_collapseMode="pin"
                        app:titleTextColor="#000000"
                />
            </android.support.design.widget.CollapsingToolbarLayout>

        </android.support.design.widget.AppBarLayout>

        <android.support.v4.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="false"
                app:layout_behavior="@string/appbar_scrolling_view_behavior"
        >
            <android.support.v7.widget.CardView
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:card_view="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    card_view:cardUseCompatPadding="true"
                    card_view:cardElevation="5dp"
                    card_view:cardCornerRadius="6dp"
                    card_view:cardBackgroundColor="#e53935"
                    android:layout_marginBottom="16dp"
            >
                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                >
                    <android.support.v7.widget.CardView
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            xmlns:card_view="http://schemas.android.com/apk/res-auto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            card_view:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp"
                            card_view:cardCornerRadius="6dp"
                            card_view:cardBackgroundColor="#f44336"
                    >
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp"
                        >
                            <TextView
                                    android:gravity="center_horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="BATSMEN"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:background="#c62828"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="1. Kane Williamson (Captain)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="2. Martin Guptill"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="3. Ross Taylor"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            xmlns:card_view="http://schemas.android.com/apk/res-auto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            card_view:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp"
                            card_view:cardCornerRadius="6dp"
                            card_view:cardBackgroundColor="#f44336"
                    >
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp"
                        >
                            <TextView
                                    android:gravity="center_horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="ALL ROUNDER"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:background="#c62828"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="4. Colin Munro (Bat All rounder)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="5. Colin de Grandhomme (Bat All rounder)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="6. James Neesham (Bat All rounder)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="7. Mitchell Santner (Bowl All rounder)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            xmlns:card_view="http://schemas.android.com/apk/res-auto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            card_view:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp"
                            card_view:cardCornerRadius="6dp"
                            card_view:cardBackgroundColor="#f44336"
                    >
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp"
                        >
                            <TextView
                                    android:gravity="center_horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="WICKET KEEPER"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:background="#c62828"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="8. Tom Blundell (WK-Batsman)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="9. Tom Latham (WK-Batsman)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="10. Henry Nicholls (WK-Batsman)"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:textColor="#000000"
                            />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            xmlns:card_view="http://schemas.android.com/apk/res-auto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            card_view:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp"
                            card_view:cardCornerRadius="6dp"
                            card_view:cardBackgroundColor="#f44336"
                    >
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp"
                        >
                            <TextView
                                    android:textColor="#000000"
                                    android:gravity="center_horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="BOWLER"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                                    android:background="#c62828"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="11. Lockie Ferguson"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="12. Trent Boult"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="13. Matt Henry"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="14. Ish Sodhi"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="15. Tim Southee"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                            <TextView
                                    android:textColor="#000000"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="15. Tim Southee"
                                    android:textStyle="bold"
                                    android:textSize="43sp"
                            />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </android.support.v4.widget.NestedScrollView>

        <android.support.design.widget.FloatingActionButton
                app:layout_anchor="@id/app_bar_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:pressedTranslationZ="12dp"
                android:src="@drawable/page_icon"
                android:elevation="6dp"
                app:layout_anchorGravity="bottom|right|end"
        />

</android.support.design.widget.CoordinatorLayout>
